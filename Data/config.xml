<?xml version='1.0' encoding='UTF-8'?>
<instance user="motion simulation">


	<!--資料來源-->
	<database ip="140.116.1.1" id="user" pwd="userpwd" table="motion"/>
	<!--ip="140.116.1.1" 資料庫所在的IP-->
	<!--id="user" pwd="userpwd" 資料庫的帳密-->
	<!--table="motion"  由資料庫中取數據的資料表-->
	
	<!--背景圖檔名稱。如有路徑也一併放在這裡-->
	<background>
		<file name="abc.jpg">
		</file>
	</background>

	<!--設定視窗的標題-->
	<setting title="simulation"/>

	<!--設定重新載入的週期-->
	<reload cycle="300" />

	<!--在視窗中顯示時鐘-->
	<clock>
		<position x="0.9" y="0" />
	</clock>


</instance>
